class Solution {
    public String reverseWords(String s) {
       int n = s.length();
       String [] str = s.split(" ");
       StringBuilder sb = new StringBuilder();
       for( int i = str.length-1 ; i>=0 ;i--){
           if(str[i] != ""){
               sb.append(" ");
               sb.append(str[i]);
               
           }
       }
       String ans = sb.toString();
       return(ans.substring(1));
    }
}
